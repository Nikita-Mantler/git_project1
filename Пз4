#include<string>
#include<iostream>
#include<stack>

using namespace std;

bool ans(string b){
  stack <char> lox;
  for (char a : b){
    switch (a){
    case '(': lox.push(')'); break;
    case '[': lox.push(']'); break;
    case '{': lox.push('}'); break;
    case ')':
    case ']':
    case '}':
      if (lox.empty() or lox.top() != a){
        return false;
      }
      lox.pop();
      break;
    default:
      break;
  }
  }
  return lox.empty();
}

int main(){
  string b;
  cin >> b;
  if (ans(b)){
    cout << "YES";
  } 
  else {
  cout << "NO";
  }
}
