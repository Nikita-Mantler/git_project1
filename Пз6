#include <iostream>
#include <vector>


using namespace std;

void filling(vector<int>& arr, int N) {
  int max, min;
  cin >> min >> max;
	for (int i = 0; i < N; ++i) {
		arr.push_back(static_cast<int>(rand() % (max-min+1) +min));
    cout << arr[i] << " ";
	}
	cout << endl;
}
void shakerSort(vector<int>& arr, int N) {
  int control = N - 1;
	int left  = 0;
	int right = N - 1;
	do {
		for (int i = left; i < right; i++) {
			if (arr[i] > arr[i + 1]) {
                swap(arr[i], arr[i + 1]);
                control = i;
			}
		}
		right = control;
		for (int i = right; i > left; i--) {
			if (arr[i] < arr[i - 1]) {
                swap(arr[i], arr[i - 1]);
                control = i;
			}
		}
		  left = control;
	} while (left < right);
}

int main()
{
  int N;
	cin >> N;
  vector<int> arr;
  arr.reserve(N);
	srand(time(0));
	filling(arr, N);
	shakerSort(arr, N); 
	for (int i = 0; i < arr.size(); i++) {
		cout << arr[i] << " ";
	}
}
