#include<string>
#include<iostream>
#include<stack>
#include <cstdio>
#include <math.h>
#include <vector>

using namespace std;

int main(){
  int b, i, z, chek = 0, count = 0;
  cin >> b;
  vector <int> v1;
  if (b > 0){
    v1.push_back(2);
    cout << 2 << " ";
  }
  for (i = 3; i <= b*b; i+=2){
    chek=0;
    for (int z : v1){
      if (i % z == 0) {
        chek++;
      } 
      if (chek>0) break;
    }
    if (v1.size() == b){
      break;
    }
    if (chek==0){
      v1.push_back(i);
      cout << i << " ";
    }
    if (v1.size() == b){
      break;
    }
  }
}
