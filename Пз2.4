#include <iostream>
#include <map>
#include <string>
#include <algorithm>
#include <vector>
#include <sstream>

using namespace std;

void mapFunc()
{
    // строим мапу чтоб хранить количество строк
    map<string, int> counts;

    string input;
    getline(cin, input);
    
    // делим строку по словам
    string word;
    for (const auto &c : input)
    {
        if (c == ' ')
        {
            // добавить слово в мапу
            ++counts[word];

            // убрать слово
            word.clear();
        }
        else
        {
            // добавить буковку слову
            word += c;
        }
    }
    for (const auto &words : counts)
    {
        cout << words.first << ": " << words.second << endl;
    }
}

void vectorFunc()
{
    srand(time(NULL));
    // задать размер векторам
    int size1, size2;
    cin >> size1 >> size2;

    // сделать два вектора и случайными числами заполнить их
    vector<int> vec1(size1), vec2(size2);
    generate(vec1.begin(), vec1.end(), []() { return rand() % 201 - 100; });
    generate(vec2.begin(), vec2.end(), []() { return rand() % 201 - 100; });

    // показать
    cout << "Vector 1:";
    for (const auto &x : vec1)
        cout << ' ' << x;
    cout << '\n';

    cout << "Vector 2:";
    for (const auto &x : vec2)
        cout << ' ' << x;
    cout << '\n';

    // сыр знач
    double mean1 = 0, mean2 = 0;
    for (const auto &x : vec1)
        mean1 += x;
    mean1 /= vec1.size();

    for (const auto &x : vec2)
        mean2 += x;
    mean2 /= vec2.size();

    cout << "Mean of vector 1: " << mean1 << '\n';
    cout << "Mean of vector 2: " << mean2 << '\n';

    // коллизия
    vector<int> intersect;
    set_intersection(vec1.begin(), vec1.end(), vec2.begin(), vec2.end(),
                          back_inserter(intersect));

    cout << "Intersecting elements:";
    for (const auto &x : intersect)
        cout << ' ' << x;
    cout << '\n';
}

// ГРАФ
void graphFunc()
{
    // кол-во вершин
    int N;
    cin >> N;
    cin.ignore();

    vector<vector<int>> graph(N);
    for (int i = 0; i < N; i++)
    {
        string line;
        getline(cin, line);
        istringstream iss(line);

        int num;
        while (iss >> num)
        {
            graph[i].push_back(num);
        }
    }
}

int main()
{
    mapFunc();
    vectorFunc();
    graphFunc();
    return 0;
}
